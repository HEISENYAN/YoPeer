.main-page{
  background-color: rgb(241,241,241);
  height: 100%;
  width:100%;
  display: block;
  position: absolute;
}
.top-margin{
  background-color: rgb(54,55,59);
  width: 100%;
}
.navigation-bar{
  background-color: rgb(54,55,59);
  width:100%;
  height: 12vh;
  display:flex;
  justify-content: center;
  align-items: center;
  color: white;
}
.notice-bar{
  background-color: white;
  height: 30vh;
  width: 92vw;
  margin-left:4vw;
  border-radius: 10px;
  display:flex;
  justify-content: center;
  align-items: center;
  z-index: 99;
  top:-3vh;
}
.swiper-container{
  width: 92vw;
  margin-left: 4vw;
}
.activity-cell{
  background-color: white;
  height: 20vh;
  width: 92vw;
  box-sizing:border-box;
  margin-left:4vw;
  margin-top: 3vh;
  border-radius: 10px;
  display:flex;
  justify-content: center;
  align-items: center;
}