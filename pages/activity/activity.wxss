.navigation-bar {
  overflow: hidden;
  background-image: url(https://i.postimg.cc/RV0p1ZvX/We-Chat6ce9dee2c0b20591e7642b3d3a163b51.jpg);
  background-size: cover; /* 可以改为 contain 或 100% 100% 以适应不同需求 */
  width: 100%;
  height: 20vh;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: white;
}
.notice-bar{
  background-color: white;
  height: 20vh;
  width: 92vw;
  margin-left:4vw;
  border-radius: 10px;
  display:flex;
  justify-content: center;
  align-items: center;
  z-index: 99;
  position: relative;
  top:-3vh;
}
.swiper-container{
  width: 92vw;
  margin-left: 4vw;
}
.activity-cell{
  background-color: white;
  height: 20vh;
  width: 92vw;
  box-sizing:border-box;
  margin-left:4vw;
  margin-top: 3vh;
  border-radius: 10px;
  display:flex;
  justify-content: center;
  align-items: center;
}