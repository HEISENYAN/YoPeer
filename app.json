      {
        "pagePath": "pages/community/community",
        "text": "社区",
        "iconPath": "icons/Picture4a.png",
        "selectedIconPath": "icons/Picture3a.png"
      },
      {
        "pagePath": "pages/personal/personal",
        "text": "个人中心",
        "iconPath": "icons/Picture6a.png",
        "selectedIconPath": "icons/Picture5a.png"
      }
    ]
  },
  "usingComponents": {
    "sky-tabbar": "/miniprogram_npm/jieyue-ui-com/sky-tabbar/sky-tabbar",
    "mp-half-screen": "/components/half-screen/half-screen",
    "sky-icon": "/miniprogram_npm/jieyue-ui-com/sky-icon/sky-icon",
    "sky-text": "/miniprogram_npm/jieyue-ui-com/sky-text/sky-text",
    "mp-slide-screen": "/components/slide-screen/slide-screen",
    "return-button": "/components/return-button/return-button",
    "yp-navigation": "/components/yp-navigation-bar/yp-navigation-bar"
