      "root": "packages/shop-package",
      "pages": [
        "pages/check-out-page/check-out-page",
        "pages/shop/shop",
        "pages/product-detail/product-detail",
        "pages/receive-info-edit/receive-info-edit",
        "pages/order-management/order-management"
      ]
    }
